type Product{
    id:ID,
    name:String
    category:String
    price:Float
    discountPercentage:Int
    priceAfterDiscount: Float
}


type Query{
    getProducts:[Product]
    getProductsByCategory(category:String):[Product]
}

type  Mutation{
    updateStock(id:ID,stock:Int):Product
    receiveNewShipment(id:ID,quantity:Int):Product
}